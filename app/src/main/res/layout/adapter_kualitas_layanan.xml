<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:padding="@dimen/dimen_16dp"
        tools:context="id.go.kemlu.legalisasidokumen.app.detailpermohonan.permohonanselesai.adapter.KualitasLayananAdapter">
        <ImageView android:layout_width="@dimen/dimen_40dp"
                   android:id="@+id/img_kualitas"
                   android:src="@drawable/ic_smile_2"
                   android:layout_height="@dimen/dimen_40dp"/>
    <TextView android:layout_width="wrap_content"
              android:id="@+id/tv_kualitas"
              android:layout_marginTop="@dimen/dimen_4dp"
              style="@style/Text.Standard"
              android:text="Cukup"
              android:layout_height="wrap_content"/>
</LinearLayout>
