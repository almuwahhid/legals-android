<?xml version="1.0" encoding="utf-8"?>
<id.go.kemlu.legalisasidokumen.utils.animationtoggle.AnimationToggle xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:id="@+id/lay_kwitansi"
              android:visibility="gone"
              android:background="@color/white"
              android:layout_height="match_parent">
              <LinearLayout android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_height="match_parent">
                  <LinearLayout android:layout_width="match_parent"
                                android:background="@color/blue_300"
                                android:paddingLeft="@dimen/dimen_16dp"
                                android:paddingRight="@dimen/dimen_16dp"
                                android:gravity="center_vertical"
                                android:paddingTop="@dimen/dimen_8dp"
                                android:paddingBottom="@dimen/dimen_8dp"
                                android:layout_height="wrap_content">
                                <ImageView android:layout_width="@dimen/dimen_48dp"
                                           android:src="@drawable/ic_logo"
                                           android:layout_height="@dimen/dimen_48dp"/>
                                <LinearLayout android:layout_width="match_parent"
                                              android:gravity="center_horizontal"
                                              android:orientation="vertical"
                                              android:layout_height="wrap_content">
                                    <TextView android:layout_width="wrap_content"
                                              style="@style/Text.Title.Banner"
                                              android:text="Pelayanan Legalisasi"
                                              android:layout_height="wrap_content"/>
                                    <TextView android:layout_width="wrap_content"
                                              android:textAlignment="center"
                                              style="@style/Text.Standard.Bold"
                                              android:text="Subdit Legalisasi, Protokol dan Konsuler"
                                              android:layout_height="wrap_content"/>
                                </LinearLayout>
                  </LinearLayout>
                  <LinearLayout android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:padding="@dimen/dimen_16dp"
                                android:layout_height="wrap_content">
                                <TextView android:layout_width="wrap_content"
                                          android:layout_gravity="center_horizontal"
                                          android:layout_marginTop="@dimen/dimen_8dp"
                                          android:textSize="@dimen/dimen_20dp"
                                          android:text="KWITANSI"
                                          style="@style/Text.Title.Banner"
                                          android:layout_height="wrap_content"/>
                      <RelativeLayout android:layout_width="match_parent"
                                      android:layout_marginBottom="@dimen/dimen_8dp"
                                      android:layout_marginTop="@dimen/dimen_8dp"
                                      android:layout_height="wrap_content">
                          <TextView android:layout_width="wrap_content"
                                    android:text="Transaksi Berhasil"
                                    android:textColor="@color/green_400"
                                    style="@style/Text.Title.Banner"
                                    android:textSize="15dp"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="wrap_content"
                                    android:text="19 Des 2019"
                                    android:textColor="@color/green_400"
                                    android:layout_alignParentRight="true"
                                    style="@style/Text.Title.Banner"
                                    android:id="@+id/tv_kwitansi_date"
                                    android:textSize="15dp"
                                    android:layout_height="wrap_content"/>
                      </RelativeLayout>
                      <LinearLayout android:layout_width="match_parent"
                                    android:layout_marginTop="@dimen/dimen_16dp"
                                    android:orientation="horizontal"
                                    android:layout_height="wrap_content">
                          <TextView android:layout_width="0dp"
                                    style="@style/Text.Standard"
                                    android:text="Telah diterima dari"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="0dp"
                                    android:layout_weight="1"
                                    style="@style/Text.Standard.Bold"
                                    android:id="@+id/tv_kwitansi_nama"
                                    android:text=": Bella Husada"
                                    android:layout_height="wrap_content"/>
                      </LinearLayout>
                      <LinearLayout android:layout_width="match_parent"
                                    android:layout_marginTop="@dimen/dimen_16dp"
                                    android:orientation="horizontal"
                                    android:layout_height="wrap_content">
                          <TextView android:layout_width="0dp"
                                    style="@style/Text.Standard"
                                    android:text="No. Permohonan"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="0dp"
                                    android:layout_weight="1"
                                    style="@style/Text.Standard.Bold"
                                    android:id="@+id/tv_kwitansi_nopermohonan"
                                    android:text=": G0009/11/06"
                                    android:layout_height="wrap_content"/>
                      </LinearLayout>
                      <LinearLayout android:layout_width="match_parent"
                                    android:layout_marginTop="@dimen/dimen_16dp"
                                    android:orientation="horizontal"
                                    android:layout_height="wrap_content">
                          <TextView android:layout_width="0dp"
                                    style="@style/Text.Standard"
                                    android:text="Total Biaya"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="0dp"
                                    android:layout_weight="1"
                                    style="@style/Text.Standard.Bold"
                                    android:id="@+id/tv_kwitansi_totalbiaya"
                                    android:text=": Rp 112.000, 00"
                                    android:layout_height="wrap_content"/>
                      </LinearLayout>
                      <LinearLayout android:layout_width="match_parent"
                                    android:layout_marginTop="@dimen/dimen_16dp"
                                    android:orientation="horizontal"
                                    android:layout_height="wrap_content">
                          <TextView android:layout_width="0dp"
                                    style="@style/Text.Standard"
                                    android:text="Jumlah Dokumen"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="0dp"
                                    android:layout_weight="1"
                                    style="@style/Text.Standard.Bold"
                                    android:id="@+id/tv_kwitansi_jumlahdokumen"
                                    android:text=": 1"
                                    android:layout_height="wrap_content"/>
                      </LinearLayout>
                      <LinearLayout android:layout_width="match_parent"
                                    android:layout_marginTop="@dimen/dimen_16dp"
                                    android:orientation="horizontal"
                                    android:layout_height="wrap_content">
                          <TextView android:layout_width="0dp"
                                    style="@style/Text.Standard"
                                    android:text="Pembayaran Transfer ke"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="0dp"
                                    android:layout_weight="1"
                                    style="@style/Text.Standard.Bold"
                                    android:id="@+id/tv_kwitansi_transferke"
                                    android:text=": Bank Mandiri"
                                    android:layout_height="wrap_content"/>
                      </LinearLayout>
                      <LinearLayout android:layout_width="match_parent"
                                    android:layout_marginTop="@dimen/dimen_16dp"
                                    android:orientation="horizontal"
                                    android:layout_height="wrap_content">
                          <TextView android:layout_width="0dp"
                                    style="@style/Text.Standard"
                                    android:text="No. Virtual Account"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"/>
                          <TextView android:layout_width="0dp"
                                    android:layout_weight="1"
                                    style="@style/Text.Standard.Bold"
                                    android:id="@+id/tv_kwitansi_va"
                                    android:text=": 0810284610009280000"
                                    android:layout_height="wrap_content"/>
                      </LinearLayout>
                  </LinearLayout>
                  <ImageView android:layout_width="wrap_content"
                             android:layout_marginTop="@dimen/dimen_16dp"
                             android:id="@+id/img_kwitansi_barcode"
                             android:layout_gravity="center_horizontal"
                             android:src="@drawable/img_barcode"
                             android:layout_height="wrap_content"/>
              </LinearLayout>
</id.go.kemlu.legalisasidokumen.utils.animationtoggle.AnimationToggle>